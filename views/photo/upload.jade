extends ../layout

block content
  
  form(method='post', action='/photo/upload', enctype='application/x-www-form-urlencoded')
    ul
      li
        label(for="title") Title 
        input#title(name="title")
      li
        label(for="body") Tags 
        i (seperated by spaces) 
        input#body(name="body")
    input(type="submit", value="Upload")
